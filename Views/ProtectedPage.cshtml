@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.ProtectedPage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "MasterProtected.cshtml";
}



<!-- Jumbotron, w title -->
  <div class="jumbotron text-center jumbotron-fluid">
     <div class="container">
          <h1 class="display-1">@Model.Value("pageTitle")</h1>
    </div>
</div>

<!--Login Status-->
<section class="wrapper">
    <div class="container">
        <div class="col-md-9">
            <article>
                @await Html.GetBlockGridHtmlAsync(Model, "bodyText")
            </article>
            <article>
                
                <br />
                @await Html.PartialAsync("partials/loginStatus")
                <br />
                
            </article>
        </div>
    </div>
</section>


<!-- Main -->
<section id="main" class="row section">
    <div class="container">
        @Model.Value("bodyText")
    </div>
</section>    


